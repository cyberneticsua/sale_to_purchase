<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="base_sales_team_wald" model="ir.config_parameter">
            <field name="key">sale_to_purchase.my_base_sales_team</field>
            <field name="value">Europe</field>
            <field name="group_ids" eval="[(4, ref('base.group_system'))]"/>
        </record>
        <record id="base_call_center_team_wald" model="ir.config_parameter">
            <field name="key">sale_to_purchase.my_base_call_center_team</field>
            <field name="value">Call Center</field>
            <field name="group_ids" eval="[(4, ref('base.group_system'))]"/>
        </record>
        <record id="base_default_activity" model="ir.config_parameter">
            <field name="key">sale_to_purchase.my_base_default_activity</field>
            <field name="value">Call</field>
            <field name="group_ids" eval="[(4, ref('base.group_system'))]"/>
        </record>
        <record id="base_default_purch_manager_email" model="ir.config_parameter">
            <field name="key">sale_to_purchase.my_base_default_purch_manager_email</field>
            <field name="value">petro@gmail.com</field>
            <field name="group_ids" eval="[(4, ref('base.group_system'))]"/>
        </record>
        <record id="base_default_activity_user" model="ir.config_parameter">
            <field name="key">sale_to_purchase.my_base_default_activity_user</field>
